# AI Security Integration API Keys
# Rename this file to .env.security_ai and add your API keys

# OpenAI API Key for GPT-4/GPT-4-Turbo
# Get your API key from https://platform.openai.com/
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic API Key for Claude models
# Get your API key from https://console.anthropic.com/
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Google Gemini API Key
# Get your API key from https://aistudio.google.com/
GEMINI_API_KEY=your_gemini_api_key_here

# Additional AI Models (Optional)
# Uncomment and add if needed
# HUGGINGFACE_API_KEY=your_huggingface_api_key_here
# COHERE_API_KEY=your_cohere_api_key_here

# AI Security Settings
# Adjust the model behavior
AI_TEMPERATURE=0.1        # Lower for more deterministic responses
AI_MAX_TOKENS=4000        # Maximum tokens in responses
AI_DEFAULT_MODEL=gpt-4    # Default model to use

# Security Analysis Settings
DETAILED_ANALYSIS=true     # Enable detailed analysis
EXPLOITATION_DETAILS=true  # Include exploitation details
REMEDIATION_CODE=true      # Include code examples in remediation

# OpenAI API Key for fallback analysis
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4-turbo

# OWASP AI Security Scanner
OWASP_AI_SCANNER_KEY=your_owasp_ai_scanner_key_here
OWASP_AI_SCANNER_URL=https://api.owasp-ai-scanner.example/scan

# Arachni AI
ARACHNI_AI_KEY=your_arachni_ai_key_here
ARACHNI_AI_URL=https://api.arachni-ai.example/scan

# AI-BOLIT
# Path to the AI-BOLIT installation
AI_BOLIT_PATH=/path/to/ai-bolit/ai-bolit.php

# Deep Exploit
DEEP_EXPLOIT_KEY=your_deep_exploit_key_here
DEEP_EXPLOIT_URL=https://api.deep-exploit.example/analyze

# SecLists AI
SECLISTS_AI_KEY=your_seclists_ai_key_here
SECLISTS_AI_URL=https://api.seclists-ai.example/generate

# AI-Driven Fuzzer
AI_FUZZER_KEY=your_ai_fuzzer_key_here
AI_FUZZER_URL=https://api.ai-fuzzer.example/fuzz

# Neural Recon
NEURAL_RECON_KEY=your_neural_recon_key_here
NEURAL_RECON_URL=https://api.neural-recon.example/scan

# AI Security Analysis API (used by AIVulnerabilityChecker)
AI_SECURITY_API_KEY=your_ai_security_api_key_here
AI_SECURITY_SERVICE_URL=https://api.ai-security-analysis.com/v1/analyze 